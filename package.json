{
  "name": "@technologiestiftung/stadtpuls-api",
  "private": true,
  "version": "2.0.0",
  "description": "An API for the stadtpuls.com project",
  "author": "ff6347 <fabianmoronzirfas@protonmail.ch>",
  "homepage": "https://github.com/technologiestiftung/stadtpuls-api/tree/main/packages/stadtpuls-api#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technologiestiftung/stadtpuls-api.git"
  },
  "files": [
    "src",
    "dist",
    "docs"
  ],
  "scripts": {
    "test": "inpyjamas-scripts test",
    "generate:types": "ts-node -r dotenv/config scripts/create-supabase-types.ts",
    "prebuild": "rimraf dist",
    "postbuild": "tsc --emitDeclarationOnly --outDir dist",
    "build": "etsc",
    "esbuild": "esbuild --platform=node --target=es2020 --format=cjs src/*.ts src/**/*.ts src/**/*.ts --outdir=dist",
    "prepare": "npm run build",
    "dev": "nodemon",
    "test:supabase:start": "docker compose --remove-orphan -f ./dev-tools/local-supabase/docker/docker-compose.yml up -d ",
    "test:supabase:stop": "docker compose -f ./dev-tools/local-supabase/docker/docker-compose.yml down"
  },
  "bugs": {
    "url": "https://github.com/technologiestiftung/stadtpuls-api/issues"
  },
  "devDependencies": {
    "@inpyjamas/scripts": "0.1.11-alpha",
    "@types/bcrypt": "3.0.1",
    "@types/faker": "5.5.3",
    "@types/node-fetch": "2.5.10",
    "@types/uuid": "8.3.0",
    "dotenv": "8.2.0",
    "esbuild": "0.11.6",
    "esbuild-node-tsc": "1.3.0",
    "eslint": "7.24.0",
    "faker": "5.5.3",
    "is-ci": "3.0.0",
    "node-fetch": "2.6.1",
    "nodemon": "2.0.7",
    "openapi-typescript": "3.2.2",
    "ts-node": "9.1.1"
  },
  "dependencies": {
    "@humanwhocodes/env": "2.2.0",
    "@technologiestiftung/fastify-supabase": "0.1.2",
    "bcrypt": "5.0.1",
    "fastify": "3.20.2",
    "fastify-auth": "1.1.0",
    "fastify-blipp": "3.1.0",
    "fastify-cors": "6.0.2",
    "fastify-helmet": "5.3.2",
    "fastify-jwt": "3.0.1",
    "fastify-plugin": "3.0.0",
    "fastify-sensible": "3.1.1",
    "fluent-json-schema": "3.0.1",
    "make-promises-safe": "5.1.0",
    "uuid": "8.3.2"
  }
}
